node {
 	// Clean workspace before doing anything
    //deleteDir()

    try {
        stage ('Clone') {
        	checkout scm
        }
        stage ('Build') {
          sh "pm2 delete npm"
          sh "pm2 start npm -- start"
        }
    } catch (err) {
        currentBuild.result = 'FAILED'
        throw err
    }
}